<div class="row">
    <div class="col s12">
        <h2 class="center-align">Student Directory</h2>
    </div>
</div>
<div class="row">
  <% @students.each do |student| %>
    <div class="col m6 s6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name %></span>
          <p>Email: <%= student.email %></p>
          <p>Event Enrollments: None</p>
        </div>
        <div class="card-action">
          <a href="#">View Profile</a>
        </div>
      </div>
    </div>
    <% end %>
  </div>