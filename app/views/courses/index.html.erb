<div class="row">
    <div class="col s12">
        <h2 class="center-align">Event Listings</h2>
    </div>
</div>
<div class="row">
  <% @courses.each do |courses| %>
    <div class="col m6 s6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= courses.short_name %>: <%= courses.name %></span>
          <p><%= courses.description %></p>
        </div>
        <div class="card-action">
          <a href="#">View</a>
          <a href="#">Manipulate</a>
        </div>
      </div>
    </div>
    <% end %>
  </div>
